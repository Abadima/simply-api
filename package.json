{
  "name": "simply-api.js",
  "version": "2.1.0",
  "description": "API Wrapper for simplyapi.js.org",
  "typings": "main.d.ts",
  "main": "main.js",
  "scripts": {
    "dev": "eslint --fix main.js eslint --fix src uglifyjs main.js --comments all -o main.cjs && npm publish --tag dev",
    "latest": "uglifyjs main.js --comments all -o main.js && npm publish --tag latest",
    "legacy": "uglifyjs main.js --comments all -o main.js npm publish --tag legacy",
    "test": "node test/chatbot.cjs && node test/tictactoe.cjs && node test/toxicity.cjs",
    "clean": "uglifyjs main.js --comments all -o main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abadima/simply-api.git"
  },
  "keywords": [
    "ai",
    "api",
    "chatbot",
    "grammar",
    "nsfw",
    "simple",
    "spelling",
    "tictactoe",
    "tic tac toe",
    "toxicity",
    "wrapper"
  ],
  "author": "Abadima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Abadima/simply-api/issues"
  },
  "homepage": "https://simplyapi.js.org",
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "uglify-js": "^3.17.4"
  },
  "engines": {
    "node": ">=16.17.0"
  }
}